Host github.com gitlab.com
  ControlMaster auto
  ControlPath ~/.ssh/sessions/%r@%h:%p
  ControlPersist 60m
  ServerAliveInterval 20

Host *.liquamine.se
  User ubuntu
  CertificateFile ~/.ssh/liquamine-eu-north-1.pem

Host *
  Protocol 2
  TCPKeepAlive yes
  Compression yes
  ServerAliveInterval 60
  HashKnownHosts yes
  UseKeychain yes
  AddKeysToAgent yes

